<!--Note: the latest verison of this file is on the web server... -->
<root>
	<feed-url>https://arseblog.com/feed</feed-url>
	<max-entries>20</max-entries>
	<item-selector>//item</item-selector>
	<item-link-selector>link</item-link-selector>
	<item-id-selector>guid</item-link-selector>
	<feed-selector>//channel</feed-selector>
	<updated-selector>//lastBuildDate</updated-selector>
	<stored-update-selector>channel lastBuildDate</stored-update-selector>
	<directory>/var/www/html/feeds/arseblog</directory>
	<feed-filename>/home/ubuntu/feeds_control/arseblog/arseblog_feed</feed-filename>
	<sections>
		<section>
			<name>Excluded</name>
			<filename>excluded.xml</filename>
			<include-titles>
				<include-title>live blog</include-title>
				<include-title>Arsecast Extra Episode</include-title>
				<include-title>Arsenal Gentleman</include-title>
				<include-title>Arsecast Episode</include-title>
			</include-titles>
			<include-contents>
			</include-contents>
			<include-authors>
			</include-authors>
			<include-links>
			</include-links>
		</section>
		<section>
			<name>Arseblog</name>
			<filename>arseblog_main.xml</filename>
			<include-all>Yes</include-all>
		</section>
	</sections>
</root>
		
	
				
